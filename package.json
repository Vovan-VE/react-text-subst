{
  "name": "react-text-subst",
  "version": "0.0.2",
  "description": "Render placeholders from text with custom React components",
  "main": "dist/cjs.js",
  "keywords": [
    "react",
    "substring",
    "replace",
    "substitute",
    "pattern",
    "placeholder"
  ],
  "author": "Vovan-VE <vovan-ve@yandex.ru>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "memoize-one": "^4.0.0",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "del-cli": "^1.0.0",
    "jest": "^19.0.2",
    "react": "~16.2.0",
    "react-dom": "~16.2.0"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vovan-ve/react-text-subst.git"
  },
  "bugs": {
    "url": "https://github.com/vovan-ve/react-text-subst/issues"
  }
